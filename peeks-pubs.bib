	file = {ACS Full Text Snapshot:C\:\\Users\\martinp23\\Zotero\\storage\\TQC6FM95\\jacs.html:text/html;Wang et al_2018_Journal of the American Chemical Society_Insights into Magneto-Optics of Helical Conjugated Polymers.pdf:G\:\\My Drive\\Chemistry\\Papers\\Zotero library\\Wang et al_2018_Journal of the American Chemical Society_Insights into Magneto-Optics of Helical Conjugated Polymers.pdf:application/pdf}
