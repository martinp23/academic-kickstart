	file = {Snapshot:C\:\\Users\\martinp23\\Zotero\\storage\\UD9X4ASL\\C7CP07348H.html:text/html;Kowalska et al_2017_Physical Chemistry Chemical Physics_Detection of a weak ring current in a nonaromatic porphyrin nanoring using.pdf:G\:\\My Drive\\Chemistry\\Papers\\Zotero library\\Kowalska et al_2017_Physical Chemistry Chemical Physics_Detection of a weak ring current in a nonaromatic porphyrin nanoring using.pdf:application/pdf}
