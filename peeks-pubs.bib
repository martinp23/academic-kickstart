	file = {ACS Full Text Snapshot:C\:\\Users\\martinp23\\Zotero\\storage\\HDX8GUPX\\acs.jpclett.html:text/html;Peeks et al_2019_The Journal of Physical Chemistry Letters_Aromaticity and Antiaromaticity in the Excited States of Porphyrin Nanorings.pdf:G\:\\My Drive\\Chemistry\\Papers\\Zotero library\\Peeks et al_2019_The Journal of Physical Chemistry Letters_Aromaticity and Antiaromaticity in the Excited States of Porphyrin Nanorings.pdf:application/pdf}
}

