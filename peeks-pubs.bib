	author = {Jeon, Intak and Peeks, Martin D. and Savagatrup, Suchol and Zeininger, Lukas and Chang, Sehoon and Thomas, Gawain and Wang, Wei and Swager, Timothy M.},
	year = {2019},
	keywords = {self-assembly, graphene, interfacial arrangement, interfacial trapping, Janus},
